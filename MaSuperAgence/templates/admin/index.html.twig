{% extends "base.html.twig" %}

{% block title 'Gerer les biens' %}
{% block body %}
    <div class="container mt-4">
        <h1>Gerer les biens</h1>
    </div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Titre</th> 
                <th>Action</th>   
            </tr>
        </thead>
        <tbody>
        {% for property in proprietes %}
            <tr>
              <td>{{property.titre}}</td>
              <td>
                <a class="btn btn-secondary" href="{{path('admin_edit', {id: property.id})}}">Editer</a>
              </td>
              </tr>
        {% endfor %}
          </tbody>
    </table>
           
                
           
    
{% endblock %}